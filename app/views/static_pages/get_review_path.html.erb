            <script>
               var tranLogoFlight = [ 'default=-52px -85px', '6E=0px -1px',
               		'S2=0px -57px', 'G8=0px -85px', 'DN=-1px -113px', 'AI=-1px -169px',
               		'9W=-38px -1px', 'IT=-38px -29px', 'SG=-38px -115px',
               		'IX=-38px -175px' ];
               var ltranLogoFlight = tranLogoFlight.length;
               
               $(document)
               		.ready(
               				function() {
               					$(".flighLogo")
               							.each(
               									function() {
               
               										var fCode = $(this).attr("name");
               										var foundFlight = 0;
               
               										if (fCode) {
               											// Check for flights in sprite
               											for (i = 0; i < ltranLogoFlight; i++) {
               												var fCodeIn = tranLogoFlight[i]
               														.split("=")[0];
               												var posCode = tranLogoFlight[i]
               														.split("=")[1];
               												if (fCodeIn == fCode) {
               													foundFlight = 1;
               													$("span", this)
               															.css(
               																	"background-position",
               																	posCode);
               													break;
               												}
               											}
               
               											// Check for flights in international airlines
               											if (!foundFlight) {
               												$("span", this)
               														.removeClass(
               																"logoFlight");
               												var imgAppend = "<img src='//cheapfaresindia.makemytrip.com/international/img/international/airline-logos/sm"+fCode+".gif'/>";
               												if ($("span", this).find(
               														"img").length == 0) {
               													$("span", this).append(
               															imgAppend);
               												}
               
               												// If the image not found
               												$("span", this)
               														.find("img")
               														.error(
               																function() {
               																	$(this)
               																			.parent(
               																					"span")
               																			.addClass(
               																					"logoFlight");
               																	$(this)
               																			.parent(
               																					"span")
               																			.css(
               																					"background-position",
               																					tranLogoFlight[0]
               																							.split("=")[1]);
               																	$(this)
               																			.remove();
               																});
               											}
               										} else {
               											if ($(this)
               													.hasClass("Multiple")) {
               												$("span", this)
               														.css(
               																"background-position",
               																"-38px -85px");
               											} else if ($(this).hasClass(
               													"Any")
               													|| $(this).hasClass(
               															"Single")) {
               												$("span", this)
               														.css(
               																"background-position",
               																tranLogoFlight[0]
               																		.split("=")[1]);
               											} else {
               												$("span", this)
               														.css(
               																"background-position",
               																tranLogoFlight[0]
               																		.split("=")[1]);
               											}
               										}
               
               									});
               
               					
               											
               				});
               
               
               
               $("#inclusion_section .tabDetails ul>li a:eq(1)").removeClass("disabled");
               $("#inclusion_section .tabDetails ul>li:eq(1) a span:eq(0)").removeClass(
               		"icoFlightDisabled");
               
               
            </script>
