    position: absolute;
    top: 15px
}

.radio input:checked+.outer .inner {
    -webkit-transform: scale(1);
    transform: scale(1);
    opacity: 1
}

.radio input:checked+.outer {
    border: 2px solid #0055b7
}

.radio input:checked+.outer .inner {
    -webkit-transform: scale(1);
    transform: scale(1);
    opacity: 1;
    background-color: #0055b7
}

.radio .outer {
    height: 18px;
    width: 18px;
    display: block;
    float: left;
    margin: 8px 10px 10px 0;
    border: 2px solid #ddd;
    border-radius: 50%;
